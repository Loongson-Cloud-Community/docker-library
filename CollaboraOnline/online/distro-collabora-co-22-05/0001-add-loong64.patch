++
++                default:
++                    if (nREG < MAX_GP_REGS)
++                    {
++                        pCppArgs[nPos] = &(gpreg[nREG]);
++                        pUnoArgs[nPos] = &(gpreg[nREG]);
++                    }
++                    else
++                    {
++                        pCppArgs[nPos] = &(ovrflw[nREG - MAX_GP_REGS]);
++                        pUnoArgs[nPos] = &(ovrflw[nREG - MAX_GP_REGS]);
++                    }
++                    nREG++;
++                    break;
++            }
++            // no longer needed
++            TYPELIB_DANGER_RELEASE(pParamTypeDescr);
++        }
