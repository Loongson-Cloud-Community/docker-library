++    try
++    {
++        try
++        {
++            callVirtualMethod(pAdjustedThisPtr, aVtableSlot.index, pCppReturn, pReturnTypeRef,
++                              bSimpleReturn, pStackStart, (pStack - pStackStart), pGPR, pFPR, nREG);
++        }
++        catch (css::uno::Exception&)
++        {
++            throw;
++        }
++        catch (std::exception& e)
++        {
