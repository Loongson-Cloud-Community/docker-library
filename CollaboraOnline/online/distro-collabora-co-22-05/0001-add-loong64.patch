++static OUString toUNOname(char const* p)
++{
++    // example: N3com3sun4star4lang24IllegalArgumentExceptionE
++
++    OUStringBuffer buf(64);
++    assert('N' == *p);
++    ++p; // skip N
++
++    while ('E' != *p)
++    {
++        // read chars count
++        long n = (*p++ - '0');
++        while ('0' <= *p && '9' >= *p)
++        {
++            n *= 10;
++            n += (*p++ - '0');
++        }
++        buf.appendAscii(p, n);
++        p += n;
++        if ('E' != *p)
++            buf.append('.');
++    }
++
++    return buf.makeStringAndClear();
++}
