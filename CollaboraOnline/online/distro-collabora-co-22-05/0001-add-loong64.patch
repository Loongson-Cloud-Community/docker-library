++                                   pThis->getBridge()->getCpp2Uno());
++            uno_destructData(pCppReturn, pReturnTypeDescr, cpp_release);
++        }
++    }
++    catch (...)
++    {
++        // fill uno exception
++        CPPU_CURRENT_NAMESPACE::fillUnoException(*ppUnoExc, pThis->getBridge()->getCpp2Uno());
