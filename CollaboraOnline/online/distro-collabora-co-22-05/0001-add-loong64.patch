++                uno_copyAndConvertData(pCppReturn, pUnoReturn, pReturnTypeDescr,
++                                       pThis->getBridge()->getUno2Cpp());
++                // destroy temp uno return
++                uno_destructData(pUnoReturn, pReturnTypeDescr, 0);
++            }
